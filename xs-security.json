{
	"xsappname": "web",
	"tenant-mode": "dedicated",
	"scopes": [{
		"name": "$XSAPPNAME.Display",
		"description": "display"
	}, {
		"name": "$XSAPPNAME.Edit",
		"description": "edit"
	}, {
		"name": "$XSAPPNAME.Delete",
		"description": "delete"
	}],

	"attributes": [{
		"name": "Country",
		"description": "Country",
		"valueType": "string"
	}],
	"role-templates": [{
		"name": "Viewer",
		"description": "View all Orders",
		"scope-references": [
			"$XSAPPNAME.Display"
		],
		"attribute-references": ["Country"]
	}, {
		"name": "Editor",
		"description": "Create, edit,delete orders",
		"scope-references": [
			"$XSAPPNAME.Edit",
			"$XSAPPNAME.Delete"
		],
		"attribute-references": [
			"Country"
		]
	}]
}