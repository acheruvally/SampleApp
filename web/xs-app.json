{
  "welcomeFile": "/web/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/web/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    },
       {
      "source": "^/core-js/(.*)$",
      "target": "$1",
      "destination": "core-backend"
    }
  ]
}